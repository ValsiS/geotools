{
    "name": "valsis/geotools",
    "type": "library",
    "description": "Geo-related tools PHP library",
    "keywords": [
      "geotools",
      "geometry",
      "geoip",
      "bounds",
      "distance",
      "geohash",
      "latlong",
      "latitude",
      "longitude"
    ],
    "license": "MIT",
    "support": {
      "issues": "https://github.com/valsis/geotools/issues",
      "source": "https://github.com/valsis/geotools"
    },
    "authors": [
      {
        "name": "Valentin Cristian",
        "email": "valsis@icloud.com",
        "role": "Developer"
      }
    ],
    "autoload": {
      "psr-4": {
        "ValsiS\\Geotools\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ValsiS\\Geotools\\Tests\\": "tests"
      }
    },
    "require": {
      "php": "^8.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
      "willdurand/geocoder": "^4.0"
    },
    "replace": {
      "toin0u/geotools": "*"
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.3-dev"
      }
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev"
  }