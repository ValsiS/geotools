{
    "name": "valsis/geotools",
    "type": "library",
    "description": "Geo-related tools PHP 7+ library",
    "keywords": ["geotools", "geometry", "geoip", "bounds", "distance", "geohash", "latlong", "latitude", "longitude"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/valsis/geotools/issues",
        "source": "https://github.com/valsis/geotools"
    },
    "authors": [
        {
            "name": "Valentin Cristian",
            "email": "valsis@icloud.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ValsiS\\Geotools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ValsiS\\Geotools\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4 || ^9.3.3",
        "willdurand/geocoder": "^4.0"
    },
    "conflict": {
        "valsis/geotools": "*"
    },
    "replace": {
        "toin0u/geotools": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
